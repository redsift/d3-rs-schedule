  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "main": "distribution/d3-rs-schedule.umd-es2015.js",
  "browser": "distribution/d3-rs-schedule.umd-es2015.min.js",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-schedule.git"
  },
  "scripts": {
